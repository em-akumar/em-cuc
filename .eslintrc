{
  "extends": "standard",
  "globals": {
    "_": false,
    "__env": false,
    "describe": false,
    "it": false
  },
  "env": {
    "es6": true
  },
  "rules": {
    "semi": [
      2,
      "always"
    ],
    "no-extra-semi": 2,
    "semi-spacing": [
      2,
      {
        "before": false,
        "after": true
      }
    ],
    "space-before-function-paren": [
      "error",
      {
        "anonymous": "always",
        "named": "never"
      }
    ]
  }
}
